{
	"name": "Lego_HTTP_to_ADLS_Themes",
	"properties": {
		"activities": [
			{
				"name": "Copy_v7a",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "themes.csv.gz"
					},
					{
						"name": "Destination",
						"value": "lego//themes.csv"
					}
				],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "GET"
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings",
							"skipLineCount": 0
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"enableSkipIncompatibleRow": true,
					"logStorageSettings": {
						"linkedServiceName": {
							"referenceName": "learnadfblobstore",
							"type": "LinkedServiceReference"
						},
						"path": "logs"
					}
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_v7a",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_v7a",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}