{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "learnadfdf"
		},
		"learnadfblobstore_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'learnadfblobstore'"
		},
		"learnadfdl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'learnadfdl'"
		},
		"HTTP_Lego_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://cdn.rebrickable.com/media/downloads/"
		},
		"learnadfdl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://learnadfstoredatalake.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "learnadfdf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"learnadfblobstore_connectionString": {
						"value": "[parameters('learnadfblobstore_connectionString')]"
					},
					"learnadfdl_accountKey": {
						"value": "[parameters('learnadfdl_accountKey')]"
					},
					"HTTP_Lego_properties_typeProperties_url": {
						"value": "[parameters('HTTP_Lego_properties_typeProperties_url')]"
					},
					"learnadfdl_properties_typeProperties_url": {
						"value": "[parameters('learnadfdl_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}